 <body>
    <div class="container">
      <div>
        <h1>Places</h1>
        <p>
          <%if @current_user%>
            <a href="/places/new" class="btn btn-primary" tabindex="-1" role="button" aria-disabled="true">Add a New Place</a>
          <%else%>
          
          <%end%>
        </p>
      </div>


      
      <div>
        <ul>
          <%if @current_user%>
            <% for place in @places %>
              <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
            <% end %>
          <%else%>
          Please log in
          <%end%>
        </ul>
      </div>
    </div>
  </body>







